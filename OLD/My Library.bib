@article{AlphasynucleinNeurodegenerativeDiseases2001,
  title = {Alpha-Synuclein and Neurodegenerative Diseases},
  author = {Goedert, Michel},
  year = {2001},
  month = jul,
  journal = {Nature Reviews Neuroscience},
  volume = {2},
  number = {7},
  pages = {492--501},
  publisher = {{Nature Publishing Group}},
  issn = {1471-0048},
  doi = {10.1038/35081564},
  abstract = {Synucleins are abundant brain proteins, the physiological functions of which are poorly understood. The synuclein family consists of three members: {$\alpha$}-, {$\beta$}- and {$\gamma$}-synuclein. {$\alpha$}- and {$\beta$}-synuclein are concentrated in nerve terminals, whereas {$\gamma$}-synuclein seems to be present throughout nerve cells. {$\alpha$}-synuclein is present in Lewy bodies, the main neuropathological characteristic of Parkinson's disease (PD). This observation has led to the proposal that {$\alpha$}-synuclein might be involved directly in the pathogenic processes that underlie PD and several other diseases; the identification of {$\alpha$}-synuclein mutations leading to the appearance of genetic forms of PD in two different families has provided support for this idea. The presence of {$\alpha$}-synuclein in Lewy bodies has also been observed in other diseases, including dementia with Lewy bodies and multiple system atrophy. Other conditions, such as Alzheimer's disease, the parkinsonism-dementia complex of Guam and Hallervorden\textendash Spatz disease, can be accompanied by the formation of {$\alpha$}-synuclein-positive Lewy bodies; however, it is not an invariant feature of these diseases. The mechanism by which {$\alpha$}-synuclein accumulation leads to neurodegeneration is not completely understood. A leading hypothesis is that the ordered assembly of {$\alpha$}-synuclein into filaments, which seems to be induced by oxidative stress, is both necessary and sufficient to cause degeneration. However, alternative ideas have not been ruled out, and it is possible that oligomeric {$\alpha$}-synuclein molecules are toxic.},
  copyright = {2001 Nature Publishing Group},
  langid = {english},
  keywords = {Animal Genetics and Genomics,Behavioral Sciences,Biological Techniques,Biomedicine,general,Neurobiology,Neurosciences},
  file = {C\:\\Users\\Raphael\\Zotero\\storage\\4C4UENB8\\Goedert - 2001 - Alpha-synuclein and neurodegenerative diseases.pdf}
}

@article{althouseSuperspreadingEventsTransmission2020,
  title = {Superspreading Events in the Transmission Dynamics of {{SARS-CoV-2}}: {{Opportunities}} for Interventions and Control},
  shorttitle = {Superspreading Events in the Transmission Dynamics of {{SARS-CoV-2}}},
  author = {Althouse, Benjamin M. and Wenger, Edward A. and Miller, Joel C. and Scarpino, Samuel V. and Allard, Antoine and {H{\'e}bert-Dufresne}, Laurent and Hu, Hao},
  year = {2020},
  month = nov,
  journal = {PLOS Biology},
  volume = {18},
  number = {11},
  pages = {e3000897},
  issn = {1545-7885},
  doi = {10.1371/journal.pbio.3000897},
  abstract = {Severe Acute Respiratory Syndrome Coronavirus 2 (SARS-CoV-2), the etiological agent of the Coronavirus Disease 2019 (COVID-19) disease, has moved rapidly around the globe, infecting millions and killing hundreds of thousands. The basic reproduction number, which has been widely used\textemdash appropriately and less appropriately\textemdash to characterize the transmissibility of the virus, hides the fact that transmission is stochastic, often dominated by a small number of individuals, and heavily influenced by superspreading events (SSEs). The distinct transmission features of SARS-CoV-2, e.g., high stochasticity under low prevalence (as compared to other pathogens, such as influenza), and the central role played by SSEs on transmission dynamics cannot be overlooked. Many explosive SSEs have occurred in indoor settings, stoking the pandemic and shaping its spread, such as long-term care facilities, prisons, meat-packing plants, produce processing facilities, fish factories, cruise ships, family gatherings, parties, and nightclubs. These SSEs demonstrate the urgent need to understand routes of transmission, while posing an opportunity to effectively contain outbreaks with targeted interventions to eliminate SSEs. Here, we describe the different types of SSEs, how they influence transmission, empirical evidence for their role in the COVID-19 pandemic, and give recommendations for control of SARS-CoV-2.},
  langid = {english},
  file = {C\:\\Users\\Raphael\\Zotero\\storage\\CJL6JR3J\\Althouse et al. - 2020 - Superspreading events in the transmission dynamics.pdf}
}

@article{alvarez-ervitiLysosomalDysfunctionIncreases2011,
  title = {Lysosomal Dysfunction Increases Exosome-Mediated Alpha-Synuclein Release and Transmission},
  author = {{Alvarez-Erviti}, Lydia and Seow, Yiqi and Schapira, Anthony H. and Gardiner, Chris and Sargent, Ian L. and Wood, Matthew J.A. and Cooper, J. Mark},
  year = {2011},
  month = jun,
  journal = {Neurobiology of Disease},
  volume = {42},
  number = {3},
  pages = {360--367},
  issn = {09699961},
  doi = {10.1016/j.nbd.2011.01.029},
  abstract = {Alpha-synuclein aggregation plays a central role in Parkinson's disease pathology. Direct transmission of alpha-synuclein from pathologically affected to healthy unaffected neurons may be important in the anatomical spread of the disease through the nervous system. We have demonstrated that exosomes released from alpha-synuclein over-expressing SH-SY5Y cells contained alpha-synuclein and these exosomes were capable of efficiently transferring alpha-synuclein protein to normal SH-SY5Y cells. Moreover, the incubation of cells with ammonium chloride or bafilomycin A1 to produce the lysosomal dysfunction recently reported in Parkinson's disease led to an increase in the release of alpha-synuclein in exosomes and a concomitant increase in alpha-synuclein transmission to recipient cells. This study clearly demonstrates the importance of exosomes in both the release of alpha synuclein and its transmission between cells and suggests that factors associated with PD pathology accelerate this process. These mechanisms may play an important role in PD pathology and provide a suitable target for therapeutic intervention.},
  langid = {english},
  file = {C\:\\Users\\Raphael\\Zotero\\storage\\5XM5345C\\Alvarez-Erviti et al. - 2011 - Lysosomal dysfunction increases exosome-mediated a.pdf}
}

